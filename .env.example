# Posty Magic Mail Club - Environment Variables
# Copy this file to .env and fill in your values
# NEVER commit the .env file to GitHub!

# RevenueCat (In-App Purchases)
EXPO_PUBLIC_VIBECODE_REVENUECAT_GOOGLE_KEY=your_revenuecat_google_key
EXPO_PUBLIC_VIBECODE_REVENUECAT_APPLE_KEY=your_revenuecat_apple_key
EXPO_PUBLIC_VIBECODE_REVENUECAT_TEST_KEY=your_revenuecat_test_key

# AI Services
EXPO_PUBLIC_VIBECODE_GOOGLE_API_KEY=your_google_gemini_api_key

# Google OAuth
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your_google_web_client_id
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=your_google_ios_client_id
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=your_google_android_client_id

# Stripe (Payments)
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
EXPO_PUBLIC_STRIPE_SECRET_KEY=your_stripe_secret_key

# Supabase (Database)
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Email (SMTP)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=465
SMTP_FROM=your_email@example.com
